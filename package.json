    "postinstall": "yarn compile",
    "release": "yarn diez release",
    "test": "lerna run test",
    "watch": "yarn diez watch",
    "prepublishOnly": "yarn clean",
    "prepack": "yarn diez minify-sources",
    "regenerate-goldens": "./scripts/regenerate-goldens.sh"
