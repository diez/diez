    "@types/klaw": "^3.0.0",
    "@types/uglify-es": "^3.0.0",
    "chalk": "^2.4.2",
    "change-case": "^3.1.0",
    "diez": "10.5.2",
    "enquirer": "^2.3.0",
    "fs-extra": "^7.0.1",
    "fs-walk": "^0.0.2",
    "glob": "^7.1.3",
    "istextorbinary": "^2.5.1",
    "klaw": "^3.0.0",
    "lerna": "^3.13.1",
    "ts-morph": "^4.3.1",
    "ts-node": "^8.1.0",
